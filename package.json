{
  "name": "@nice-digital/global-nav",
  "version": "4.1.0",
  "description": "Global header and footer components for NICE digital services",
  "main": "lib/index.js",
  "types": "./types.d.ts",
  "scripts": {
    "// LOCAL DEV": "",
    "start": "webpack serve --mode development --hot --inline --config webpack.config.js",
    "start:nohot": "cross-env HOT=false webpack serve --mode development",
    "// PRODUCTION BUILDS": "",
    "build": "webpack --mode=production --config webpack.config.prod.js",
    "es5ify": "cross-env NODE_ENV=production babel src -d lib --extensions \".js\",\".jsx\",\".snap\" --copy-files --no-copy-ignored --ignore \"src/**/*.test.js\",\"src/**/*.test.jsx\",\"src/setupTests.js\",\"src/polyfill/**/*.js\",\"src/cdn.js\",\"src/**/*.snap\"",
    "// TESTS": "",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "jest --notify",
    "test:unit:watch": "jest --watch",
    "test:unit:coverage": "npm run test:unit -- --coverage",
    "// LINTING": "",
    "lint": "npm run prettier && npm run lint:js && npm run lint:scss",
    "prettier": "prettier --check .",
    "prettier:fix": "npm run prettier -- --write",
    "lint:js": "eslint .",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint:scss": "stylelint **/*.scss",
    "lint:scss:fix": "npm run lint:scss -- --fix",
    "// LIFECYCLE HOOKS": "",
    "prepare": "npm run es5ify",
    "// TEAMCITY": "",
    "audit:teamcity": "npm audit --audit-level=moderate --json|npm-audit-reporter-teamcity"
  },
  "author": "NICE Digital Services (https://github.com/nice-digital/global-nav/graphs/contributors)",
  "contributors": [
    "NICE Digital Services (https://www.nice.org.uk/)",
    "Ian Routledge (https://www.ediblecode.com)"
  ],
  "homepage": "https://github.com/nice-digital/global-nav",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nice-digital/global-nav.git"
  },
  "license": "MIT",
  "browserslist": [
    "extends @nice-digital/browserslist-config"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "engines": {
    "node": ">= 12"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.8",
    "@babel/core": "^7.16.12",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-member-expression-literals": "^7.16.7",
    "@babel/plugin-transform-property-literals": "^7.16.7",
    "@babel/plugin-transform-react-jsx": "^7.16.7",
    "@babel/plugin-transform-reserved-words": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.16.10",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@nice-digital/browserslist-config": "^2.0.1",
    "@nice-digital/eslint-config": "^1.0.3",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "autoprefixer": "^10.4.2",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.3",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^8.1.1",
    "core-js": "^3.20.3",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.7",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "enzyme-to-json": "^3.6.2",
    "es3ify-loader": "^0.2.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-webpack-plugin": "^2.6.0",
    "eslint-plugin-react": "^7.29.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-teamcity-reporter": "^0.9.0",
    "mockdate": "^3.0.5",
    "npm-audit-reporter-teamcity": "^0.3.1",
    "postcss": "^8.4.5",
    "postcss-loader": "^5.2.0",
    "prettier": "^2.5.1",
    "promise-polyfill": "^8.2.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-refresh": "^0.10.0",
    "sass": "^1.49.0",
    "sass-loader": "^11.1.1",
    "source-map-loader": "^2.0.1",
    "style-loader": "^2.0.0",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.21.0",
    "stylelint-webpack-plugin": "^2.3.2",
    "terser-webpack-plugin": "^5.3.0",
    "webpack": "^5.67.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^3.11.3",
    "webpack-merge": "^5.7.3"
  },
  "dependencies": {
    "@babel/runtime": "^7.16.7",
    "@mantine/hooks": "^4.2.12",
    "@nice-digital/icons": "^2.2.7",
    "@nice-digital/nds-container": "^1.0.11",
    "@nice-digital/nds-core": "^1.3.1",
    "accessible-autocomplete": "^2.0.3",
    "classnames": "^2.3.1",
    "es5-polyfill": "0.0.6",
    "js-cookie": "^2.2.1",
    "prop-types": "^15.8.1",
    "unfetch": "^4.2.0"
  },
  "files": [
    "lib",
    "*.d.ts"
  ],
  "directories": {
    "lib": "lib"
  },
  "volta": {
    "node": "14.18.1"
  }
}
