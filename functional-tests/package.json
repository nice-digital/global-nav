    "@types/node": "^20.9.0",
    "@wdio/allure-reporter": "^7.9.0",
    "@wdio/cli": "^7.9.0",
    "@wdio/cucumber-framework": "^7.9.0",
    "@wdio/local-runner": "^7.19.7",
    "@wdio/selenium-standalone-service": "^7.8.0",
